{
  "_version": "1.12.0",
  "sap.app": {
    "id": "com.tw.mypr.My_custom_pr",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "sourceTemplate": {
      "id": "ui5template.basicSAPUI5ApplicationProject",
      "version": "1.40.12"
    },
    "dataSources": {
      "mainService": {
        "uri": "https://mydemo21.teamwork.net/sap/opu/odata/SAP/Z_PURREQUISITION_EXT_SRV/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml"
        }
      },
      "ValueHelpColumns": {
        "uri": "model/ValueHelpColumns.json",
        "type": "JSON"
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
      "viewName": "com.tw.mypr.My_custom_pr.view.Init",
      "type": "XML",
      "async": true,
      "id": "Init"
    },
    "dependencies": {
      "minUI5Version": "1.65.6",
      "libs": {
        "sap.ui.layout": {},
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.unified": {},
        "sap.uxap": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.tw.mypr.My_custom_pr.i18n.i18n"
        }
      },
      "": {
        "preload": true,
        "dataSource": "mainService",
        "settings": {
          "useBatch": true,
          "user": "QTNGO",
          "password": "Dangerfi0",
          "metadataUrlParams": {
            "sap-documentation": "heading,quickinfo"
          }
        }
      },
      "VHCols": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "ValueHelpColumns"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.tw.mypr.My_custom_pr.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "PRList",
          "pattern": "",
          "target": [
            "PRList"
          ]
        },
        {
          "name": "PRDetail",
          "pattern": "PRDetail/{PreqNo}",
          "target": [
            "PRDetail"
          ]
        },
        {
          "name": "itemDetail",
          "pattern": "itemDetail/{PreqItem}/:edit:",
          "target": [
            "ItemDetail"
          ]
        },
        {
          "name": "DocumentFlow",
          "pattern": "DocumentFlow/{PreqNo}/{PreqItem}",
          "titleTarget": "",
          "greedy": false,
          "target": [
            "DocumentFlow"
          ]
        }
      ],
      "targets": {
        "PRList": {
          "viewType": "XML",
          "viewName": "PRList"
        },
        "PRDetail": {
          "viewType": "XML",
          "viewName": "PRDetail"
        },
        "ItemDetail": {
          "viewType": "XML",
          "viewName": "ItemDetail"
        },
        "DocumentFlow": {
          "viewType": "XML",
          "viewName": "DocumentFlow"
        }
      }
    }
  }
}